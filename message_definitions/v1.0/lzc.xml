<?xml version="1.0" encoding="UTF-8"?>
<mavlink>
<!-- Landing Zones Canada, Inc: -->
<!-- mavlink ID range: 20000 - 20099 -->
<!-- Command range:    20000 - 20049 -->
<!-- Status  range:    20050 - 20099 -->

<!--================================================================================================-->
<!-- Enums -->
<!--================================================================================================-->
  <enums>
      <!--================================================================================================-->
      <!--LZC specific Command Enums -->
      <!--================================================================================================-->
      <enum name="PAYLOAD_CONTROL_FLAGS" bitmask="true">
        <description>These encode the payload control flags and sent as part of the MAV_CMD_PAYLOAD_CTRL message.</description>
        <entry value="1" name="PAYLOAD_CONTROL_FLAGS_RESERVED1">
          <description>Reserved - Future use.</description>
        </entry>
        <entry value="2" name="PAYLOAD_CONTROL_FLAGS_RESERVED2">
          <description>Reserved - Future use.</description>
        </entry>
        <entry value="4" name="PAYLOAD_CONTROL_FLAGS_RESERVED3">
          <description>Reserved - Future use.</description>
        </entry>
        <entry value="8" name="PAYLOAD_CONTROL_FLAGS_RESERVED4">
          <description>Reserved - Future use.</description>
        </entry>
        <entry value="16" name="PAYLOAD_CONTROL_SWEEP_WING_PERCENTAGE">
          <description>Valid value in command when bit set ON</description>
        </entry>
        <entry value="32" name="PAYLOAD_CONTROL_SWEEP_WING_CALIBRATE">
          <description>Valid value in command when bit set ON</description>
        </entry>
        <entry value="64" name="PAYLOAD_CONTROL_SET_MODEM_BOOST">
          <description>Valid value in command when bit set ON</description>
        </entry>
        <entry value="128" name="PAYLOAD_CONTROL_SET_BEACON_LIGHTS">
          <description>Valid value in command when bit set ON</description>
        </entry>
      </enum>
    <!--================================================================================================-->
    <!--LZC specific Status Enums -->
    <!--================================================================================================-->
    <enum name="PAYLOAD_STATUS_FLAGS" bitmask="true">
      <description>These encode the payload status and sent as part of the PAYLOAD_STATUS message.</description>
      <entry value="1" name="PAYLOAD_STATUS_FLAGS_RESERVED4">
        <description>Reserved - Future use.</description>
      </entry>
      <entry value="2" name="PAYLOAD_STATUS_FLAGS_RESERVED3">
        <description>Reserved - Future use.</description>
      </entry>
      <entry value="4" name="PAYLOAD_STATUS_FLAGS_SWEEP_WING_LIMIT">
        <description>Sweep Wing Limit Status (Pin 54) - on when set.</description>
      </entry>
      <entry value="8" name="PAYLOAD_STATUS_FLAGS_HSTM_POWER">
        <description>HSTM Power status (Pin 105) - on when set.</description>
      </entry>
      <entry value="16" name="PAYLOAD_STATUS_FLAGS_BEACON_LIGHTS">
        <description>Beacon lights status (Pin 102) - on when set.</description>
      </entry>
      <entry value="32" name="PAYLOAD_STATUS_FLAGS_POSITION_LIGHTS">
        <description>Position lights status (Pin 101) - on when set.</description>
      </entry>
      <entry value="64" name="PAYLOAD_STATUS_FLAGS_BALLOON_RELEASE">
        <description>Balloon release status (Pin 104) - on when set.</description>
      </entry>
      <entry value="128" name="PAYLOAD_STATUS_FLAGS_PARACHUTE_RELEASE">
        <description>Parachute Deploy status (Pin 103) - on when set.</description>
      </entry>
    </enum>
  </enums>


<!--================================================================================================-->
<!--Control Messages (20000 - 20049) -->
<!--================================================================================================-->
<messages>
    <!-- Payload Control Message -->
    <message id="20000" name="PAYLOAD_CTRL">
      <description>Control payload devices.</description>
      <field type="uint8_t" name="flags" enum="PAYLOAD_CONTROL_FLAGS" display="bitmask">Payload control bitmap flags</field>
      <field type="uint8_t" name="beacon_lights">Beacon lights payload</field>
      <field type="uint8_t" name="modem_boost">Boost vehicle modem payload</field>
      <field type="uint8_t" name="sweep_wing_calibrate">Sweep wing calibration flag</field>
      <field type="uint8_t" name="sweep_wing_percentage">Seep wing percentage value</field>
    </message>

<!--================================================================================================-->
<!--Status Messages (20050 - 20099) -->
<!--================================================================================================-->

    <!-- IMET Mavlink Message -->
    <message id="20050" name="IMET_SENSOR_RAW">
      <description>Contains raw sensor data from IMET application.</description>
      <field type="uint32_t" name="time_boot_ms">Timestamp (milliseconds since system boot)</field>  
      <field type="uint8_t" name="data_length">Data length.</field>
      <field type="uint8_t[64]" name="data">IMET sensor raw data message</field>
    </message>
    <!-- Payload Status Message -->
    <message id="20051" name="PAYLOAD_STATUS">
      <description>Contains Payload status information.</description>
      <field type="uint8_t" name="flags" enum="PAYLOAD_STATUS_FLAGS" display="bitmask">Payload status bitmap flags</field>
      <field type="uint8_t" name="uplink_quality">Uplink quality scaled in factor 0 to 255</field>
      <field type="uint16_t" name="servo_vcc" units="mV">Servo rail voltage.</field>
      <field type="int16_t" name="support_board_temp" units="cdegC">Vehicle support board temperature</field>
      <field type="uint16_t" name="sweep_wing" units="cdeg">Sweep Wing Angle in Degrees.</field>
    </message>
  </messages>
</mavlink>